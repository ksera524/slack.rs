# Slack設定
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# ログ設定
# ログレベル (trace, debug, info, warn, error)
RUST_LOG=info

# ログ形式: JSONL (JSON Lines) 形式固定
# 1行に1つのJSONオブジェクトを出力し、ログ解析ツールやストリーミング処理に最適化

# ログオプション
NO_COLOR=false          # カラー出力を無効化する場合はtrue
LOG_TARGET=false        # モジュール名を表示する場合はtrue
LOG_THREAD=false        # スレッド情報を表示する場合はtrue
LOG_LINE=false          # 行番号を表示する場合はtrue

# 特定モジュールのログレベル設定例
# RUST_LOG=slack=debug,tower_http=trace,info

# 本番環境向け設定例
# RUST_LOG=info,slack::service=debug
# LOG_TARGET=true
# LOG_LINE=false

# ログ解析ツール向け設定例
# RUST_LOG=info
# LOG_TARGET=true
# LOG_THREAD=true
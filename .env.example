# Slack設定
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# ログ設定
# ログレベル (trace, debug, info, warn, error)
RUST_LOG=info

# ログ形式 (json, pretty, compact)
LOG_FORMAT=pretty

# ログオプション
NO_COLOR=false          # カラー出力を無効化する場合はtrue
LOG_TARGET=false        # モジュール名を表示する場合はtrue
LOG_THREAD=false        # スレッド情報を表示する場合はtrue
LOG_LINE=false          # 行番号を表示する場合はtrue

# 特定モジュールのログレベル設定例
# RUST_LOG=slack=debug,tower_http=trace,info

# 本番環境向け設定例
# LOG_FORMAT=json
# RUST_LOG=info,slack::service=debug
# NO_COLOR=true